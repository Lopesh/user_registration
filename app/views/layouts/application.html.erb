<!DOCTYPE html>
<html>
  <head>
    <title>UserRegistration</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  
  </head>

  <body  class="loading" data-layout-mode="horizontal" data-layout='{"mode": "light", "width": "fluid", "menuPosition": "fixed", "topbar": {"color": "light"}}'>
    
    <div id="wrapper">
      <% if user_signed_in? %>
        <%= render "top_header"%>
        <%= render partial: "application/top_sub_nav" %>
      <% end %>
      <div class="content-page">
        <div class="content">

          <div class="container-fluid">
            
            <%= yield %>
          </div>

        </div>
        
      </div>
    </div>
    
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <%= javascript_pack_tag 'admin','data-turbolinks-track': 'reload' %>
    <%= yield :javascript %>
  </body>
</html>
